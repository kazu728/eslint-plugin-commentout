{
  "name": "@kzmat/eslint-plugin-commentout",
  "version": "0.1.0",
  "description": "Eslint plugin, disallow commentout except annotation comment",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "kzmat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kzmat/eslint-plugin-commentout"
  },
  "bugs": "https://github.com/kzmat/eslint-plugin-commentout/issues",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.lib.json",
    "clean": "rimraf ./dist",
    "fix:eslint": "eslint --fix --ext ts,tsx ./src",
    "fix:prettier": "prettier --write \"./**/*.{md,json}\"",
    "lint:eslint": "eslint --ext ts,tsx ./src",
    "lint:prettier": "prettier --check \"./**/*.{md,json}\"",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "^8.4.3",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^28.1.2",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/experimental-utils": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-local-rules": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12.*"
  },
  "eslintIgnore": [
    "/.eslintrc.js",
    "eslint-local-rules.js",
    "jest.config.js"
  ]
}
